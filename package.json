{
  "name": "datagum",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "deploy": "turbo deploy",
    "deploy:web": "turbo deploy --filter=web",
    "deploy:queue": "turbo deploy --filter=queue-worker",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "cf-typegen": "turbo cf-typegen",
    "db:generate": "turbo db:generate --filter=web",
    "db:push": "turbo db:push --filter=web",
    "db:studio": "turbo db:studio --filter=web",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "eslint": "^9",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
